
@model CumulativeProject2.Models.Teacher
@{
    ViewBag.Title = "Show";
}

<a class="navlink" href="/Teacher/List">Back To Teachers</a>
<div id="containershow" class="my-3">
    <h2>@Model.teacherFname @Model.teacherLname</h2>
    <p>Employee Number: @Model.employeenumber</p>
    <p>Salary: @Model.salary</p> 
    <p>Date Hired: @Convert.ToDateTime(Model.hiredate).ToString("dd MMM yyyy")</p>
</div>